<div class="container">
  <div class="col-xs-12 col-md-6 col-md-offset-3">
    <h1 class="content-page-h1">Training Session Log</h1>
    <div class="dotted_hr"></div>
    <% @all_my_sessions.each do |session| %>
      <div class="exercise_set_container">
        <%= link_to session.open == false ? training_session_path(session) : session_sets_path do %>
          <div class="session_history_bullet">
            <%= image_tag "icons_symbols/tr_arrow_right_a.svg", style: "width: 30px;" %> <%= session.created_at.to_date %>
            <div style="margin: 0 0 0 35px;">
              <%= session.session_strategy.name %>
              <%= "<span class='training_session_index_active_notice'>[active]</span>".html_safe if session.open == true %>
            </div>
          </div>
        <% end %>
      </div>
    <% end %>
  </div>
</div>
